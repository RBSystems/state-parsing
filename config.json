[
    {
        "name": "room-update.py",
        "triggers": [
            {
                "kind": "daily",
                "at": "14:45:00-06:00"
            },
            {
                "kind": "interval",
                "every": "5m"
            },
            {
                "kind": "match",
                "match": {
                    "hostname": "teton",
                    "event": {
                        "eventInfoKey": "button.*",
                        "type": "CORE"
                    }
                }
            }
        ],
        "enabled": true
    }
]
